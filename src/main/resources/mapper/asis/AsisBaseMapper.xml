<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mig.repository.asis.AsisBaseMapper">

    <select id="selectAsisSysdate" parameterType="HashMap" resultType="HashMap">
        SELECT /* Asis */ TO_CHAR(SYSDATE, 'YYYY/MM/DD HH24:MI:SS') AS DT
        FROM DUAL
    </select>

</mapper>